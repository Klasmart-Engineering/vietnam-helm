    "env": "prod",

    "prometheus_host": "http://prometheus-kube-prometheus-prometheus.monitoring.svc.cluster.local:9090",
    "desired_pods_getter": "redis",
    
    "helm_prometheus": {
        "enabled": false
    },
    "helm_fluentbit": {
        "enabled": false
    },
    "helm_cert_manager": {
        "enabled": true
    },
    "helm_persistence": {
        "enabled": false
    },
    "helm_nginx_ingress": {
        "enabled": true
    },   
    "helm_ecr_token_vietnam": {
        "enabled": false
    },   
    "helm_ecr_token_infra": {
        "enabled": true
    },
    "helm_db_backup": {
        "enabled": false,
        "endpoint": "",
        "region": "",
        "bucket": ""
    },

    "gcp": {
        "mysql": {
            "name":      "kidsloop-mysql-99999",
            "version":   "MYSQL_5_7",
            "tier":      "db-n1-standard-4",
            "charset":   "utf8mb4",
            "collation": "utf8mb4_bin",
            "user":      "kidsloop-mysql",
            "assessment_user": "mysql-assessment-user",
            "disk": {
                "size": 10, 
                "type": "PD_SSD"
            }
        },
        "postgresql": {
            "name": "kidsloop-postgresql-99999",
            "assessment_database": "assessment",
            "version":   "POSTGRES_11",
            "tier":      "db-custom-4-15360",
            "charset":   "UTF8",
            "collation": "en_US.UTF8",
            "user":      "kidsloop-postgresql",
            "disk": {
                "size": 10, 
                "type": "PD_SSD"
