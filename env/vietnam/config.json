{
    "provider": "bizflycloud",
    "k8s_namespace_monitoring": "monitoring",
    "k8s_namespace_kidsloop": "okc",
    "k8s_namespace_persistence": "persistence",
    "k8s_storage_class": "premium-hdd",
    "kl_domain": "-beta.kidsloop.vn",

    "helm_cert": {
        "enabled": true
    },
    "helm_prometheus": {
        "enabled": true
    },
    "helm_fluentbit": {
        "enabled": true
    },
