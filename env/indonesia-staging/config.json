{
    "provider": "gcp",
    "terraform_bucket": "kidsloop-indonesia-terraform",
    "k8s_namespace_kidsloop": "okc",
    "k8s_service_type": "NodePort",
    "kl_domain": "-beta.kidsloop.id",
    "domain": "kidsloop.id",

    "helm_prometheus": {
        "enabled": false
    },
    "helm_fluentbit": {
        "enabled": false
    },
    "helm_cert_manager": {
        "enabled": false
    },
    "helm_redis": {
        "enabled": false
    },
    "helm_postgresql": {
        "enabled": false
    },    
    "helm_mysql": {
        "enabled": false
    },   
    "helm_nginx_ingress": {
        "enabled": false
    },   
    "helm_ecr_token_vietnam": {
        "enabled": false
    },   
        
    "gcp": {
        "mysql": {
            "name":      "kidsloop-mysql-99998",
            "version":   "MYSQL_5_7",
            "tier":      "db-f1-micro",
            "charset":   "utf8mb4",
            "collation": "utf8mb4_bin",
            "user":      "kidsloop-mysql",
            "disk": {
                "size": 10, 
                "type": "PD_SSD"
            }
        },
        "postgresql": {
            "name": "kidsloop-postgresql-99999",
            "version":   "POSTGRES_11",
            "tier":      "db-f1-micro",
            "charset":   "UTF8",
            "collation": "en_US.UTF8",
            "user":      "kidsloop-postgresql",
            "disk": {
                "size": 10, 
                "type": "PD_SSD"
            }
        },
        "redis": {
            "name": "kidsloop-99999",
            "display_name": "kidsloop",
            "version": "REDIS_5_0",
            "memory_size": 1,
            "tier": "STANDARD_HA"
        }
    },

    "storage": "",
    
    "static_repository": "494634321140.dkr.ecr.ap-northeast-2.amazonaws.com/vietnam-hub",
    "static_pull_secret": "ecr-registry",
    
    "base_url_user": "https://api-beta.kidsloop.id/user/",
    "base_url_h5p": "https://h5p.kidsloop.net",

    "image_versions": {
        "kl2_static": "ecedc13-indonesia-staging",
        "sfu_manager": "a7e5b75"
    }
}
