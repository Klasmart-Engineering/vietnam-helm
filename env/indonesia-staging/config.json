{
    "provider": "gcp",
    "terraform_bucket": "kidsloop-indonesia-terraform",
    "k8s_namespace_kidsloop": "okc",
    "k8s_service_type": "NodePort",
    "kl_domain": ".beta.kidsloop.id",
    "domain": "beta.kidsloop.id",
    "env": "beta",

    "prometheus_host": "http://prometheus-kube-prometheus-prometheus.monitoring.svc.cluster.local:9090",
    "desired_pods_getter": "redis",
    
    "helm_prometheus": {
        "enabled": false
    },
    "helm_fluentbit": {
        "enabled": false
    },
    "helm_cert_manager": {
        "enabled": true
    },
    "helm_persistence": {
        "enabled": false
    },
    "helm_nginx_ingress": {
        "enabled": true
    },   
    "helm_ecr_token_vietnam": {
        "enabled": false
    },   
    "helm_ecr_token_infra": {
        "enabled": true
    },
    "helm_db_backup": {
        "enabled": false,
        "endpoint": "",
        "region": "",
        "bucket": ""
    },

    "gcp": {
        "mysql": {
            "name":      "kidsloop-mysql-99998",
            "version":   "MYSQL_5_7",
            "tier":      "db-f1-micro",
            "charset":   "utf8mb4",
            "collation": "utf8mb4_bin",
            "user":      "kidsloop-mysql",
            "assessment_user": "mysql-assessment-user",
            "disk": {
                "size": 10, 
                "type": "PD_SSD"
            }
        },
        "postgresql": {
            "name": "kidsloop-postgresql-99999",
            "assessment_database": "assessment",
            "version":   "POSTGRES_11",
            "tier":      "db-f1-micro",
            "charset":   "UTF8",
            "collation": "en_US.UTF8",
            "user":      "kidsloop-postgresql",
            "disk": {
                "size": 10, 
                "type": "PD_SSD"
            }
        },
        "redis": {
            "name": "kidsloop-99999",
            "display_name": "kidsloop",
            "version": "REDIS_5_0",
            "memory_size": 1,
            "tier": "STANDARD_HA"
        }
    },

    "whitelabel_storage_provider": "google",
    "whitelabel_storage_bucket": "whitelabel-kl-id-beta",

    "storage": {
        "endpoint": "https://s3.ap-southeast-1.amazonaws.com",
        "region": "ap-southeast-1",
        "bucket": "klid-beta-cms-res",
        "protocol": "s3",
        "download_mode": "cloudfront",
        "cdn_path": "https://res.beta.kidsloop.id",
        "cdn_live": "https://res.beta.kidsloop.id"
