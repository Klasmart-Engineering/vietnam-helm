{
    "provider": "gcp",
    "terraform_bucket": "kl-indonesia-terraform",
    "k8s_namespace_monitoring": "monitoring",
    "k8s_namespace_kidsloop": "okc",
    "k8s_namespace_persistence": "persistence",
    "k8s_storage_class": "standard",
    "kl_domain": "-oc.kidsloop.vn",

    "helm_cert": {
        "enabled": false
    },
    "helm_prometheus": {
        "enabled": false
    },
    "helm_fluentbit": {
        "enabled": false
    },
    "helm_redis": {
        "enabled": false
    },
    "helm_postgresql": {
        "enabled": false
    },
    "helm_mysql": {
        "enabled": false
    },


    "gcp": {
        "mysql": {
            "name":      "kidsloop-mysql-99995",
            "version":   "MYSQL_5_7",
            "tier":      "db-f1-micro",
            "charset":   "utf8mb4",
            "collation": "utf8mb4_bin",
            "user":      "kidsloop-mysql",
            "disk": {
                "size": 10, 
                "type": "PD_SSD"
            }
        },
        "postgresql": {
            "name": "kidsloop-postgresql-99994",
            "version":   "POSTGRES_11",
            "tier":      "db-f1-micro",
            "charset":   "utf8mb4",
            "collation": "utf8mb4_bin",
            "user":      "kidsloop-postgresql",
            "disk": {
                "size": 10, 
                "type": "PD_SSD"
            }
        }
    }
}