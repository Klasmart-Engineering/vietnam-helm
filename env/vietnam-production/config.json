{
    "provider": "vngcloud",
    "k8s_namespace_kidsloop": "klvn",
    "k8s_service_type": "ClusterIP",
    "kl_domain": ".kidsloop.vn",
    "domain": "kidsloop.vn",

    "redis_host": "10.0.2.9",
    "postgresql_host": "10.0.2.4",
    "postgresql_username": "kidslooproot",
    "postgresql_database": "kidsloop",
