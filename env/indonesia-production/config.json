{
    "provider": "gcp",
    "terraform_bucket": "kidsloop-indonesia-terraform-prod",
    "k8s_namespace_kidsloop": "okc",
    "k8s_service_type": "ClusterIP",
    "kl_domain": ".kidsloop.id",
    "domain": "kidsloop.id",
    "deployment_region": "indonesia",
    "deployment_name": "indonesia-production",
    "vault_addr": "https://vault.devops.klpsre.com",
    "vault_path": "customer/indonesia-production",
    "env": "production",

    "prometheus_host": "http://prometheus-kube-prometheus-prometheus.monitoring.svc.cluster.local:9090",
    "desired_pods_getter": "redis",

    "helm_prometheus": {
        "enabled": true
    },
    "helm_kube_monitoring": {
        "enabled": true
    },
    "helm_cert_manager": {
        "enabled": true
    },
    "helm_persistence": {
        "enabled": false
    },
    "helm_nginx_ingress": {
        "enabled": true
    },   
    "helm_ecr_token_vietnam": {
        "enabled": false
    },   
    "helm_ecr_token_infra": {
        "enabled": true
    },
    "helm_db_backup": {
        "enabled": false,
        "endpoint": "",
        "region": "",
        "bucket": ""
    },

    "gcp": {
        "mysql": {
            "name":      "kidsloop-mysql-99999",
            "version":   "MYSQL_5_7",
            "tier":      "db-custom-2-13312",
            "charset":   "utf8mb4",
            "collation": "utf8mb4_bin",
            "user":      "kidsloop-mysql",
            "assessment_user": "mysql-assessment-user",
            "disk": {
                "size": 10, 
                "type": "PD_SSD"
            }
        },
        "postgresql": {
            "name": "kidsloop-id-prod-postgresql",
            "assessment_database": "assessment",
            "attendance_database": "attendance_service_db",
            "xapi_database": "xapidbrecord",
            "version":   "POSTGRES_12",
            "tier":      "db-custom-2-13312",
            "charset":   "UTF8",
            "collation": "en_US.UTF8",
            "user":      "kidsloop-postgresql",
            "disk": {
                "size": 10, 
                "type": "PD_SSD"
            }
        },
        "redis": {
            "name": "kidsloop-99999",
            "display_name": "kidsloop",
            "version": "REDIS_5_0",
            "memory_size": 1,
            "tier": "STANDARD_HA"
        }
    },

    "whitelabel_storage_provider": "google",
    "whitelabel_storage_bucket": "whitelabel-kl-id-prod",

    "kube_monitoring": {
        "slack_channel": "#sre-production-alerts"
    },
    "storage": {
        "endpoint": "https://s3.ap-southeast-1.amazonaws.com",
        "region": "ap-southeast-1",
        "bucket": "klid-prod-cms-res",
        "pdf_bucket": "klid-prod-pdf",
        "protocol": "s3",
        "download_mode": "cloudfront",
        "cdn_path": "https://res.kidsloop.id",
        "cdn_live": "https://res.kidsloop.id"
    },

    "h5p_storage": {
        "endpoint": "https://s3.ap-southeast-1.amazonaws.com",
